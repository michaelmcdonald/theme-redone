{
  "block_meta": {
    "BLOCK_REGISTER_NAME": "title-subtitle",
    "BLOCK_TITLE": "Title Subtitle",
    "keywords": ["title", "subtitle"],
    "hasSidebar": true,
    "hasGlobalSettings": true,
    "hasExample": true
  },
  "attributes": {
    "title": {
      "type": "object",
      "field_meta": {
        "type": "text",
        "variation": "short",
        "label": "Title",
        "max_chars": 60
      },
      "default": {
        "text": ""
      }
    },

    "text": {
      "type": "object",
      "field_meta": {
        "type": "text",
        "variation": "rich",
        "label": "Content",
        "max_chars": 650
      },
      "default": {
        "text": ""
      }
    },

    "inspector_block_margin": {
      "type": "string",
      "default": "no-margin"
    },

    "inspector_bg_is_dark": {
      "type": "boolean",
      "default": false
    },
    "inspector_section_padding": {
      "type": "string",
      "default": "sec-pad-md"
    },
    "inspector_section_padding_top": {
      "type": "boolean",
      "default": false
    },
    "inspector_section_padding_btm": {
      "type": "boolean",
      "default": true
    }
  }
}
